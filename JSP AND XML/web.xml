<?xml version="1.0" encoding="UTF-8"?>


<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">

	<servlet>
		<servlet-name>WonItemDetailsInsert</servlet-name>
		<servlet-class>com.payment.WonItemDetailsInsertServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WonItemDetailsInsert</servlet-name>
		<url-pattern>/winsert</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>WonDetails</servlet-name>
		<servlet-class>com.payment.WonListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WonDetails</servlet-name>
		<url-pattern>/wondetails</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>PaymentList</servlet-name>
		<servlet-class>com.payment.PaymentListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PaymentList</servlet-name>
		<url-pattern>/plist</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Paymentlogin</servlet-name>
		<servlet-class>com.payment.PaymentLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Paymentlogin</servlet-name>
		<url-pattern>/plog</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>PaymentDetailsInsert</servlet-name>
		<servlet-class>com.payment.PaymentDetailsInsertServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PaymentDetailsInsert</servlet-name>
		<url-pattern>/pinsert</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>PaymentUpdateDetails</servlet-name>
		<servlet-class>com.payment.PaymentUpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PaymentUpdateDetails</servlet-name>
		<url-pattern>/pupdate</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DeletePayment</servlet-name>
		<servlet-class>com.payment.PaymentDeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeletePayment</servlet-name>
		<url-pattern>/pdelete</url-pattern>
	</servlet-mapping>


	</web-app>